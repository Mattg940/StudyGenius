{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "build/**"]
		},
		"dev": {
			"cache": false
		},
		"lint": {},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**", "tmp/**"]
		},
		"typecheck": {},
		"e2e": {
			"dependsOn": ["build"],
			"cache": false
		}
	}
}

